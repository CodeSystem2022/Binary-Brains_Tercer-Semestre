package package1;

public class Clase1 {
    
    // Modificador de acceso Public
    public String atributoPublic = "Valor atributo publico";
    
    // Constructor Publico
    public Clase1() { System.out.println("Constructos Publico");}    
    
    // Metodos    
    public void metodoPublico(){System.out.println("Metodo Publico");}
}
